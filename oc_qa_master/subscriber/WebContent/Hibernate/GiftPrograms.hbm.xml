<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="org.mq.marketer.campaign.beans.GiftPrograms" table="gift_programs">
	  
        <id name="giftProgramId" type="java.lang.Long" column="gift_program_id">
            <generator class="native"/>
        </id>
        <property name="giftProgramName" column="gift_program_name" type="string"/>
        <property name="programStatus" column="program_status" type="string"/>
        <property name="userId" column="user_id" type="java.lang.Long"/>
        <property name="createdDate" column="created_date" type="calendar"/>
        <property name="modifiedDate" column="modified_date" type="calendar"/>
        <property name="expiryInMonths" column="expiry_in_months" type="java.lang.Long"/>
        <property name="redemptionType" column="redemption_type" type="string"/>
        <property name="pinLength" column="pin_length" type="java.lang.Long"/>
        <property name="cardType" column="card_type" type="string"/>
	  
 </class>
</hibernate-mapping>
