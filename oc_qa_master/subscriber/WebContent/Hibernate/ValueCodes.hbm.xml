<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.mq.marketer.campaign.beans.ValueCodes"
		table="value_codes">
	

		<id name="Id" type="java.lang.Long" column="id">
			<generator class="native" />
		</id>
		<property name="ValuCode" type="string">
			<column name="value_code"  />
		</property>
		<property name="associatedWithFBP" type="boolean">
			<column name="associated_with_FBP"  default ="false"/>
		</property>
		
		<property name="Description" type="string">
			<column name="description"  />
		</property>
		
		<property name="OrgId" type="java.lang.Long">
			<column name="org_id"  />
		</property>
		
		<property name="CreatedBy" type="string">
			<column name="created_by"  />
		</property>
		
		<property name="ModifiedBy" type="string">
			<column name="modified_by"  />
		</property>
		
		<property name="CreatedDate" type="calendar">
			<column name="created_date"  />
		</property>
	
		<property name="ModifiedDate" type="calendar">
			<column name="modified_date"  />
		</property>
	
				
	
	</class>
	
	
	
</hibernate-mapping>
