<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="org.mq.marketer.campaign.beans.PromoTrxLog"
	table="promo_trx_log">
	<id name="id" type="java.lang.Long" column="id">
		<generator class="native" />
	</id>
	<property name="requestId" type="string">
		<column name="request_id" />
	</property>
	<property name="jsonRequest" type="text">
		<column name="json_request" />
	</property>
	<property name="jsonResponse" type="text">
		<column name="json_response" />
	</property>
	<property name="type" type="string">
		<column name="type" />
	</property>
	<property name="mode" type="string">
		<column name="mode" />
	</property>
	<property name="pcFlag" type="boolean">
		<column name="pcflag" />
	</property>
	<property name="requestDate" type="calendar">
		<column name="request_date" />
	</property>
	
	
	<property name="userDetail" type="string">
		<column name="user_detail" />
	</property>
	<property name="docSID" type="string">
		<column name="doc_sid" />
	</property>
	<property name="storeNumber" type="string">
		<column name="store_number" />
	</property>
	
	<property name="customerId" type="string">
		<column name="customer_id" />
	</property>
	
	
	
	
	</class>
</hibernate-mapping>
