<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.mq.marketer.campaign.beans.FAQ"
		table="faq">
	
		<id name="faqId" type="java.lang.Long">
			<column name="faq_id" />
			<generator class="native" />
		</id> 
		
		<property name="userId" type="long">
			<column name="user_id" />
		</property>
		
		<property name="orgId" type="java.lang.Long">
			<column name="org_id" />
		</property>
		
		<property name="language" type="string">
			<column name="language" />
		</property> 
		
		<property name="faqContent" type="text">
			<column name="faq_content" />
		</property>
		
		<property name="termsAndCondition" type="text">
			<column name="terms_and_condition" />
		</property>
		
		<!-- <property name="faqSpanishContent" type="text">
			<column name="faq_spanish_content" />
		</property>
		
		<property name="spanishTermsAndCondition" type="text">
			<column name="spanish_terms_and_condition" />
		</property> -->
		
		<property name="createdDate" type="calendar">
			<column name="created_date"  />
		</property>
		
		</class>
</hibernate-mapping>
