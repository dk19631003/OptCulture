<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="org.mq.marketer.campaign.beans.LoyaltyMemberSessionID" table="loyalty_member_sessionID">

    <id name="id" type="java.lang.Long">
      <column name="id"/>
      <generator class="native"/>
    </id>
    <property name="cardNumber" type="string">
            <column name="card_number" />
   </property>
   <property name="createdDate" type="calendar">
       <column name="created_date"/>
    </property>
     <property name="sessionID" type="string">
		<column name="session_id"  />
	</property>
    
      <property name="userId" type="java.lang.Long">
       <column name="user_id"/>
    </property>
    <property name="orgID" type="java.lang.Long">
       <column name="org_id"/>
    </property>
    <property name="status" type="string">
		<column name="status"  />
	</property>
    <property name="deviceID" type="string">
		<column name="device_id"  />
		</property>
	</class>
</hibernate-mapping>