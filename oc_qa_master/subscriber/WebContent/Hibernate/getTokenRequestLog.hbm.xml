<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.mq.marketer.campaign.beans.GetTokenRequestLog"
		table="get_token_request_log">
		<id name="id" type="java.lang.Long" column="id">
			<generator class="native" />
		</id>
		
		<property name="jsonRequest" type="string">
			<column name="json_request" />
		</property>
		<property name="jsonResponse" type="string">
			<column name="json_response" />
		</property>
		
		<property name="requestDate" type="calendar">
			<column name="request_date" />
		</property>
		
		</class>
</hibernate-mapping>
