	<window id="manualAdditionWinId" border="normal" visible="false" width="500px" height="200px" closable="true" title="Add To Suppression" onClose="self.setVisible(false); event.stopPropagation();">
			<radiogroup id="manualAdditionChoice"/>
			
			<h:table cellspacing="0" cellpadding="4" width="100%" height="100%"  bgcolor="#FFFFFF" style="padding:2px;border:0px solid #cccccc;border-radius:5px;">
			        <h:tr>
