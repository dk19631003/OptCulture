<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.mq.captiway.scheduler.beans.SMSCampaignSentUrlShortCode"
		table="sms_campaign_sent_url_short_code">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="native" />
		</id>
		
		<property name="smsCampaignUrlId" type="java.lang.Long">
			<column name="sms_campaign_Url_id" />
		</property>
		<property name="sentId" type="java.lang.Long">
			<column name="sentId" />
		</property>
		<property name="generatedShortCode" type="java.lang.String">
			<column name="generatedShortCode" length="20" />
		</property>
		<property name="originalShortCode" type="java.lang.String">
			<column name="originalShortCode" length="20" />
		</property>
		
	</class>
</hibernate-mapping>