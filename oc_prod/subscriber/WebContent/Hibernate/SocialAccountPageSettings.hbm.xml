<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.mq.marketer.campaign.beans.SocialAccountPageSettings"
		table="social_account_page_settings">
		
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="native" />
		</id>
		
		<property name="userId" type="java.lang.String">
			<column name="user_id" length="255" not-null="true" />
		</property>
		
		<property name="profilePageName" type="java.lang.String">
			<column name="profile_page_name" length="255" not-null="true" />
		</property>
		
		<property name="profilePageId" type="java.lang.String">
			<column name="profile_page_id" length="255" not-null="true" />
		</property>
		
		<property name="profilePageType" type="java.lang.String">
			<column name="profile_page_type" length="255" not-null="true" />
		</property>
		
	</class>
</hibernate-mapping>