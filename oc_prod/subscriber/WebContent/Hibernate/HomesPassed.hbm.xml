		</property>

		<property name="addressOne" type="string">
			<column name="address_one"   length="100"
				 />
		</property>
		<property name="addressTwo" type="string">
			<column name="address_Two" length="100"  />
		</property>

		<property name="street" type="string">
			<column name="street" length="100"   />
		</property>


		<property name="area" type="string">
			<column name="area" length="100" />
		</property>

		<property name="city" type="string">
			<column name="city"  length="50"   />
		</property>
		<property name="district" type="string">
			<column name="district"  length="100"   />
		</property>
		
		<property name="zip" type="string">
			<column name="zip"   />
		</property>
		
		<property name="state" type="string">
			<column name="state" length="50"   />
		</property>
		
		<property name="country" type="string">
			<column name="country" length="50"   />
		</property>
		<property name="udf1" type="string">
			<column name="udf1" length="30"  />
		</property>
		
       <property name="udf2" type="string">
