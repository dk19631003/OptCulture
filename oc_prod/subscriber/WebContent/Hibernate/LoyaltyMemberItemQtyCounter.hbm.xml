<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.mq.marketer.campaign.beans.LoyaltyMemberItemQtyCounter"
		table="loyalty_membership_item_quantinty">
	
		<id name="id" type="java.lang.Long" column="id">
			<generator class="native" />
		</id>
		<property name="loyaltyID" type="java.lang.Long">
			<column name="loyalty_id"  />
		</property>
		
		<property name="SPRuleID" type="java.lang.Long">
			<column name="sp_rule_id"  />
		</property>
		
		<property name="itemStr" type="string">
			<column name="item_str"  />
		</property>
					
		<property name="userId" type="java.lang.Long">
			<column name="used_id"  />
		</property>
		
		<property name="orgId" type="java.lang.Long">
			<column name="org_id"  />
		</property>
		
	<property name="qty" type="double">
			<column name="qty" default="0" />
		</property>
	
	</class>
	
	
	
</hibernate-mapping>
