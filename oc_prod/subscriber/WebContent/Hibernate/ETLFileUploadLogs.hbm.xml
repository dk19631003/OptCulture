<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.mq.marketer.campaign.beans.ETLFileUploadLogs" table="etl_fileupload_logs">
  
    <id name="logId" type="java.lang.Long" column="log_id">
      <generator class="native"/>
    </id>
    <property name="userId" type="long">
		<column name="user_id" />
	</property>
 	<property name="fileName" type="string">
		<column name="file_name" />
	</property>
	<property name="uploadTime" type="calendar">
		<column name="upload_time" />
	</property>
	
	<property name="recordCount" type="int">
		<column name="record_count" />
	</property>
	
	<property name="fileStatus" type="string">
		<column name="file_status" />
	</property>
	
	<property name="comments" type="string" >
		<column name="comments" />
	</property>
	
	<property name="processedFilePath" type="string" >
		<column name="processedFilePath" />
	</property>
	
	<property name="receiptDetailsPath" type="string" >
		<column name="receiptDetailsPath" />
	</property>
	
	
	
  </class>
</hibernate-mapping>
