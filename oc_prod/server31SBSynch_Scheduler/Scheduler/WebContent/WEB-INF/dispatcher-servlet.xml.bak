		<property name="externalSMTPEventsProcessor">
			<ref bean="externalSMTPEventsProcessor" />
		</property>
		<property name="campaignReportEventsQueue">
			<ref bean="campaignReportEventsQueue" />
		</property>
		
		
	</bean>
	
