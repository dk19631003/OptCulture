<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.mq.captiway.scheduler.beans.SMSBounces"
		table="sms_bounces">
		<id name="bounceId" type="java.lang.Long">
			<column name="bounce_id" />
			<generator class="native" />
		</id>
		<many-to-one name="sentId"
			class="org.mq.captiway.scheduler.beans.SMSCampaignSent"
			foreign-key="click_sentFkey" fetch="select" lazy="false">
			<column name="sent_id" not-null="true" />
		</many-to-one>
		<property name="category" type="string">
			<column name="category" />
		</property>
		<property name="mobile" type="string">
			<column name="mobile" />
		</property>
		<property name="message" type="text">
			<column name="message" />
		</property>
		<property name="bouncedDate" type="calendar">
			<column name="date" />
		</property>
		<property name="crId" type="long">
	      <column name="cr_id"/>
	    </property>
	</class>
</hibernate-mapping>
